int a;
int b;
int c;

int greenTicket(int a, int b, int c) {
  if(a==b && b==c){
    return 20;
  }
  if(a==b || b==c || c==a){
    return 10;
  }
  else
  return 0;
}

void setup(){
  int x = greenTicket(1,2,3);
  println(x);
  
  int y = greenTicket(2,2,2);
  println(y);
  
  int z = greenTicket(1,1,2);
  println(z);
}
